{
  "name": "webex-meeting-widget",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@webex/widgets": "1.20.6",
    "eslint-config-react-app": "7.0.0",
    "gh-pages": "3.2.3",
    "parcel": "2.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "webex": "1.150.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:widget": "parcel build src/index.js --no-source-maps",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "alias": {
    "@webex/internal-plugin-user": "/node_modules/@webex/internal-plugin-user",
    "@webex/helper-html": "/node_modules/@webex/helper-html",
    "@webex/webex-core": "/node_modules/@webex/webex-core",
    "@webex/internal-plugin-device": "/node_modules/@webex/internal-plugin-device",
    "@webex/common": "/node_modules/@webex/common"
  }
}
